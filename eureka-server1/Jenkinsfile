node{
            stage('checkout'){
                    git branch: 'config-server', url: 'https://github.com/hand-wanggang/erueka-demo.git'
            }
            stage('build') {
                    echo 'Building'
                    sh "gradle clean bootRepackage"
            }
            stage('Test') {
                steps {
                    echo 'Testing'
                }
            }
 }